{
    "schemaVersion": 1,
    "id": "blame",
    "version": "${version}",

    "name": "Blame",
    "description": "Crashing due to a feature or structure from a mod? Don't worry!\nThis mod will find exactly which feature/structure and biome the crash is occurring from!\nTIME TO PLACE SOME BLAME!!!",
    "authors": [
        "TelepathicGrunt"
    ],
    "contact": {
        "homepage": "https://www.curseforge.com/minecraft/mc-mods/blame-fabric",
        "sources": "https://github.com/TelepathicGrunt/Blame/tree/Fabric-1.17.x"
    },

    "license": "LGPLv3",
    "icon": "assets/blame/blame_icon.png",

    "environment": "*",
    "entrypoints": {
        "main": [
            "com.telepathicgrunt.blame.Blame"
        ],
        "client": [
            "com.telepathicgrunt.blame.Blame"
        ],
        "server": [
            "com.telepathicgrunt.blame.Blame"
        ]
    },
    "mixins": [
        "blame.mixins.json"
    ],

    "depends": {
        "fabricloader": ">=0.11.3",
        "minecraft": "1.17.x"
    },
    "suggests": {},
    "custom": {
        "notify_gradle_properties_url": "https://raw.githubusercontent.com/TelepathicGrunt/Blame/Fabric-1.17.x/gradle.properties",
        "notify_gradle_properties_key": "mod_version"
    }
}
